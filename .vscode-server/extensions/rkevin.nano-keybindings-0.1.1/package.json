{
	"name": "nano-keybindings",
	"displayName": "Nano keybindings",
	"description": "Nano keybindings for VSCode",
	"repository": "https://github.com/rkevin-arch/vscode-nano-keybindings",
	"version": "0.1.1",
	"publisher": "rkevin",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:nano-keybindings.cut",
		"onCommand:nano-keybindings.toggleCutToEnd"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nano-keybindings.cut",
				"title": "Cut (nano)"
			},
			{
				"command": "nano-keybindings.toggleCutToEnd",
				"title": "Toggle Cut to End (nano)"
			}
		],
		"keybindings": [
			{
				"command": "nano-keybindings.cut",
				"key": "ctrl+k"
			},
			{
				"command": "nano-keybindings.toggleCutToEnd",
				"key": "alt+k"
			},
			{
				"command": "editor.action.clipboardPasteAction",
				"key": "ctrl+u"
			},
			{
				"command": "actions.find",
				"key": "ctrl+w"
			},
			{
				"command": "editor.action.startFindReplaceAction",
				"key": "ctrl+\\"
			},
			{
				"command": "editor.action.nextMatchFindAction",
				"key": "alt+w"
			},
			{
				"command": "editor.action.previousMatchFindAction",
				"key": "alt+q"
			},
			{
				"command": "workbench.action.gotoLine",
				"key": "ctrl+shift+_"
			},
			{
				"command": "workbench.action.closeActiveEditor",
				"key": "ctrl+x"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.53.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"__metadata": {
		"id": "e5f90019-a429-41d5-9760-6d2e661c79f2",
		"publisherId": "a41df8db-c46b-4935-97c8-d06d7eb932e0",
		"publisherDisplayName": "rkevin",
		"targetPlatform": "undefined",
		"isApplicationScoped": false,
		"updated": false,
